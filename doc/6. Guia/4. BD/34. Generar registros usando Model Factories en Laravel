            factory(User::class)->create([
                        'name' => 'Klvst3r',
                        'email' => 'klvst3r@email.com',
                        'password' => bcrypt('laravel'),
                        'profession_id' => $professionId,
                        'is_admin' => true,
                   ]);

            factory(User::class)->create([
                'profession_id' => $professionId
            ]);


            factory(User::class)->create();


---

